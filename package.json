{
  "name": "reservation-service",
  "version": "1.0.0",
  "description": "Reservation Service Module for Air Tree n Tree",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "eslint": "eslint .",
    "webpack-dev": "webpack --config client/config/webpack.config.dev.js",
    "webpack-prod": "webpack --config client/config/webpack.config.prod.js",
    "webpack-server": "webpack-dev-server --config client/config/webpack.config.dev.js",
    "babel": "babel client/src -d build/client/src && babel server/src -d build/server/src",
    "start": "npm run babel && node build/server/src/index.js",
    "dev": "concurrently \"babel -w client/src -d build/client/src\" \"babel server/src -d build/server/src\" \"nodemon build/server/src/index.js\"",
    "seed": "node build/server/src/database/seed.js",
    "seed-dev": "env NODE_ENV=development npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Air-Tree-n-Tree/Reservation-Service.git"
  },
  "author": "Jin Cai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Air-Tree-n-Tree/Reservation-Service/issues"
  },
  "homepage": "https://github.com/Air-Tree-n-Tree/Reservation-Service#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-enzyme": "^7.1.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "moment": "^2.25.3",
    "mongoose": "^5.9.13",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
